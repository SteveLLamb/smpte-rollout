<!DOCTYPE html>
<html lang="en">

  {{>header}}

    <!-- Main content starts - do not edit above this line -->
    <table id="sorttable" class="table table-small table-hover table-bordered display">
      <thead class="thead-light">
        <tr>
          <th scope="col">Region</th>
          <th scope="col" class="text-center">m49</th>
          <th scope="col" class="text-center">ISO Alpha 2</th>
          <th scope="col" class="text-center">Total Countries</th>
          <th scope="col" class="text-center">Total Site Count</th>
          <th scope="col" class="text-center">SMPTE Site %</th>
        </tr>
      </thead>
      <tbody id="searchTable">
        {{#data}}
        <tr id="{{../idType}}-{{m49}}">
          <td class="region">
            {{region}}
          </td>
          <td class="m49 text-center">
            <code>{{m49}}</code>
          </td>
          <td class="isoAlpha2 text-center">
            <code>{{isoAlpha2}}</code>
          </td>
          <td class="countryCount text-center">
            {{countryCount}}
          </td>
          <td class="siteCount text-center">
            {{siteCount}}
          </td>
          <td class="smpteSite text-center">
            {{#if smpteSite}}
              {{smpteSite}}%
            {{/if}}
          </td>
        </tr>
        {{/data}}
      </tbody>
    </table>
    <!-- Main content ends - do not edit below this line -->
{{>footer}}

</html>